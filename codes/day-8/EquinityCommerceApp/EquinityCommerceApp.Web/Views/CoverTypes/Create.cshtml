
@model CoverTypeModel
@{
    ViewData["Title"] = "Create";
}

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary"> Create Cover Type</h2>
        </div>
        <div class="mb-3">
            <label asp-for="Name"></label>
            <input asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>                
        <button class="btn btn-primary" style="width:150px">Create</button>
        &nbsp;&nbsp;
        <a asp-action="Index" asp-controller="CoverTypes" class="btn btn-secondary" style="width:150px">Back To List</a>
    </div>
</form>


